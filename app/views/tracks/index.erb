<div class="row">
  <% @tracks.each do |track| %>
  <% @track = track %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img src="images/hand.png">
        <div class="caption clearfix">
          <div class='caption-text'>
            <h3><a href="/tracks/<%= track.id %>"><%= track.artist %></a></h3>
            <p><%= track.song_title %></p>
          </div>
          <%= erb :'tracks/up_vote' %>
          <div class="upvote_count">
            <%= track.upvote_count %> likes
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>

